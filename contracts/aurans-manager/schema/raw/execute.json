{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "description": "Message type for `execute` entry_point",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "update_config"
      ],
      "properties": {
        "update_config": {
          "type": "object",
          "required": [
            "admin",
            "name_code_id",
            "resolver_code_id"
          ],
          "properties": {
            "admin": {
              "type": "string"
            },
            "name_code_id": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "resolver_code_id": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  ]
}
